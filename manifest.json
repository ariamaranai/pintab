{
  "manifest_version": 3,
  "name": "pintab",
  "version": "0",
  "description": "Ctrl+Q: Pin tabs | Ctrl+W: Disable closing pinned tabs",
  "icons": {
    "128": "128.png"
  },
  "commands": {
    "0": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Pinned tabs"
    },
    "1": {
      "suggested_key": {
        "default": "Ctrl+W"
      },
      "description": "Closed tabs"
    }
  },
  "background": {
    "service_worker": "bg.js"
  }
}